{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true,
        },
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathSuffix": "src/app",
            "rootPathPrefix": "~/app"
          },
          {
            "rootPathSuffix": "src",
            "rootPathPrefix": "~/"
          },
        ]
      }
    ]
  ]
}
